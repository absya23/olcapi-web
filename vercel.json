{
    "version": 2,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.4.1" }
    },
    "builds": [
        {
            "src": "public/index.php",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/(.*)\\.(.*)$",
            "dest": "public/$1.$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_KEY": "base64:FoDq79WdE1o210p3mUOuE+gnbSFpJyBDuN9IJY28Q5c=",
        "APP_URL": "https://www.laravel-example.vercel.app"
    }
}
